{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covidadlf"
		},
		"ls_ablob_covidlearn1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidlearn1'"
		},
		"ls_adls_covidlearnadls1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidlearnadls1'"
		},
		"ls_adls_covidlearnadls1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidlearnadls1.dfs.core.windows.net"
		},
		"ls_httpreq_opendata_cedc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/77dee7b2-772f-4da6-892c-54c57212df65/resourceGroups/covidpoc/providers/Microsoft.Storage/storageAccounts/covidlearn1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covidadlf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidlearn1_connectionString": {
						"value": "[parameters('ls_ablob_covidlearn1_connectionString')]"
					},
					"ls_adls_covidlearnadls1_accountKey": {
						"value": "[parameters('ls_adls_covidlearnadls1_accountKey')]"
					},
					"ls_adls_covidlearnadls1_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidlearnadls1_properties_typeProperties_url')]"
					},
					"ls_httpreq_opendata_cedc_properties_typeProperties_url": {
						"value": "[parameters('ls_httpreq_opendata_cedc_properties_typeProperties_url')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}